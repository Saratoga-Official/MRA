        "建造次数": {
            "type": "input",
            "inputs": [
                {
                    "name": "建造次数",
                    "default": "1",
                    "pipeline_type": "int",
                    "verify": "^[1-9][0-9]*$",
                    "pattern_msg": "必须为正整数"
                }
            ],
            "pipeline_override": {
                "开始自动建造": {
                    "max_hit": "{建造次数}"
                }
            }
        },
        "若无空位使用快速建造工具": {
